{
  "name": "reuniverse-web",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world && fpack build ./lib/js/webapp.bs.js --development --output=dist",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world && rm -rf dist",
    "test": "bsb -make-world && jest",
    "refmt": "bsrefmt --in-place webapp.re ./**/*.re*"
  },
  "keywords": [
    "BuckleScript",
    "Reason",
    "ReasonReact"
  ],
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^6.0.0",
    "fastpack": "^0.8.4"
  },
  "dependencies": {
    "bs-fetch": "^0.5.0",
    "reason-promise": "^1.0.2",
    "reason-react": "^0.7.0"
  }
}
